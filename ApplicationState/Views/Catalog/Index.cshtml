@model System.Collections.Generic.Dictionary<int, string>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/site.css" rel="stylesheet" />
    <title>List</title>
</head>
<body>
    <ul>
        @foreach (var item in Model)
        {
            <li><a asp-action="details" asp-controller="catalog" asp-route-id="@item.Key">@item.Value</a></li>
        }
    </ul>
    <br />
    <a asp-controller="home" asp-action="index">главная</a>
</body>
</html>
